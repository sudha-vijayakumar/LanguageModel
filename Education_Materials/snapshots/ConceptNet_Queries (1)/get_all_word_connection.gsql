CREATE QUERY get_all_word_connection(VERTEX<word> p) FOR GRAPH ConceptNet SYNTAX V2 { 
  /******************************************
	* Grabing all connections of a specified Patient
  * Sample Parameter: word
	******************************************/
  ListAccum<EDGE> @@edgeList;
	seed = {p};
	
	S1 = SELECT s
	       FROM seed:s -(:e)-:t
	       ACCUM @@edgeList += e;
	
  PRINT  @@edgeList AS edges; 
}